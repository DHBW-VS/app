    "@capacitor/clipboard": "4.1.0",
    "@capacitor/core": "4.6.1",
    "@capacitor/ios": "4.6.1",
    "@capacitor/keyboard": "4.1.0",
    "@capacitor/preferences": "4.0.2",
    "@capacitor/share": "4.1.0",
    "@capacitor/splash-screen": "4.1.2",
    "@capacitor/status-bar": "4.1.1",
    "@capawesome-team/capacitor-file-opener": "0.0.2",
    "@fortawesome/angular-fontawesome": "0.10.2",
    "@fortawesome/fontawesome-svg-core": "6.1.1",
    "@fortawesome/free-brands-svg-icons": "6.1.1",
    "@fortawesome/free-regular-svg-icons": "6.1.1",
    "@fortawesome/free-solid-svg-icons": "6.1.1",
    "@fullcalendar/core": "5.11.3",
    "@fullcalendar/daygrid": "5.11.3",
    "@fullcalendar/icalendar": "5.11.3",
    "@fullcalendar/interaction": "5.11.3",
    "@fullcalendar/timegrid": "5.11.3",
    "@ionic/angular": "6.0.5",
    "@ionic/pwa-elements": "3.1.1",
    "@ngneat/elf": "2.1.0",
    "@ngneat/elf-cli-ng": "1.0.0",
    "@ngneat/elf-devtools": "1.2.1",
    "@ngneat/elf-entities": "4.4.2",
    "@ngneat/elf-persist-state": "1.1.5",
    "buffer": "6.0.3",
    "cordova-plugin-advanced-http": "3.2.2",
    "cordova-plugin-file": "6.0.2",
    "date-fns": "2.29.3",
    "hammerjs": "2.0.8",
    "leaflet": "1.9.3",
    "rxjs": "7.4.0",
    "stream": "0.0.2",
    "timers": "0.1.1",
    "tslib": "2.0.3",
    "xml2js": "0.4.23",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "13.0.3",
    "@angular-devkit/build-angular": "15.0.5",
    "@angular-eslint/builder": "13.0.1",
    "@angular-eslint/eslint-plugin": "13.0.1",
    "@angular-eslint/eslint-plugin-template": "13.0.1",
    "@angular-eslint/schematics": "15.1.0",
    "@angular-eslint/template-parser": "13.0.1",
    "@angular/cli": "15.0.5",
    "@angular/compiler": "15.0.4",
    "@angular/compiler-cli": "15.0.4",
    "@angular/language-service": "15.0.4",
    "@capacitor/cli": "4.6.1",
    "@cypress/schematic": "2.4.0",
    "@ionic/angular-toolkit": "6.0.0",
    "@ionic/cli": "6.20.6",
    "@types/jest": "27.4.0",
    "@types/jest-when": "3.5.0",
    "@types/leaflet": "1.9.0",
    "@types/node": "18.11.18",
    "@types/xml2js": "0.4.11",
    "@typescript-eslint/eslint-plugin": "5.48.0",
    "@typescript-eslint/parser": "5.48.0",
    "cy-mobile-commands": "0.3.0",
    "cypress": "12.3.0",
    "eslint": "8.31.0",
    "eslint-plugin-unicorn": "45.0.2",
