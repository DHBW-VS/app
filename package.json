    "@capacitor/core": "4.6.1",
    "@capacitor/ios": "4.6.1",
    "@capacitor/keyboard": "4.1.0",
    "@capacitor/preferences": "4.0.2",
    "@capacitor/share": "4.1.0",
    "@capacitor/splash-screen": "4.1.2",
    "@capacitor/status-bar": "4.1.1",
    "@capawesome-team/capacitor-file-opener": "0.0.2",
    "@fortawesome/angular-fontawesome": "0.14.1",
    "@fortawesome/fontawesome-svg-core": "6.5.1",
    "@fortawesome/free-brands-svg-icons": "6.5.1",
    "@fortawesome/free-regular-svg-icons": "6.5.1",
    "@fortawesome/free-solid-svg-icons": "6.5.1",
    "@fullcalendar/core": "6.1.10",
    "@fullcalendar/daygrid": "6.1.10",
    "@fullcalendar/icalendar": "6.1.10",
    "@fullcalendar/interaction": "6.1.10",
    "@fullcalendar/timegrid": "6.1.10",
    "@ionic/angular": "7.6.6",
    "@ionic/core": "7.6.6",
    "@ionic/pwa-elements": "3.2.2",
    "@ngneat/elf": "2.5.0",
    "@ngneat/elf-cli-ng": "1.0.0",
    "@ngneat/elf-devtools": "1.3.0",
    "@ngneat/elf-entities": "5.0.0",
    "@ngneat/elf-persist-state": "1.2.0",
    "buffer": "6.0.3",
    "cordova-plugin-advanced-http": "3.2.2",
    "cordova-plugin-file": "6.0.2",
    "date-fns": "3.3.1",
    "hammerjs": "2.0.8",
    "leaflet": "1.9.4",
    "rxjs": "7.5.0",
    "stream": "0.0.2",
    "swiper": "9.2.3",
    "timers": "0.1.1",
    "tslib": "2.0.3",
    "xml2js": "0.6.2",
    "zone.js": "0.14.3"
  },
  "devDependencies": {
    "@angular-builders/jest": "17.0.0",
