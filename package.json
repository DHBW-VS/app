    "jest-when": "3.5.0",
    "prettier": "2.8.1",
    "prettier-plugin-organize-imports": "3.2.1",
    "semver": "7.3.8",
    "standard-version": "9.5.0",
    "ts-jest": "27.1.3",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.2",
    "typescript": "4.8.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dhbw-vs/app.git"
  },
  "bugs": {
    "url": "https://github.com/dhbw-vs/app/issues"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-file": {},
      "cordova-plugin-advanced-http": {
        "OKHTTP_VERSION": "3.10.0"
      }
    }
  },
  "standard-version": {
    "scripts": {
      "postbump": "node scripts/bump-native-versions.js && git add android/app/build.gradle ios/App/App/Info.plist"
    }
  },
  "elf": {
    "cli": {
      "repoTemplate": "class",
      "plugins": [
        "@ngneat/elf-cli-ng"
      ],
      "repoLibrary": "src/app/store"
    }
  }
}
