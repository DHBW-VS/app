    "jest-preset-angular": "14.0.0",
    "jest-when": "3.6.0",
    "prettier": "3.2.4",
    "prettier-plugin-organize-imports": "3.2.4",
    "semver": "7.5.4",
    "standard-version": "9.5.0",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.1.2",
    "typescript": "5.3.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dhbw-vs/app.git"
  },
  "bugs": {
    "url": "https://github.com/dhbw-vs/app/issues"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-file": {},
      "cordova-plugin-advanced-http": {
        "OKHTTP_VERSION": "3.10.0"
      }
    }
  },
  "standard-version": {
    "scripts": {
      "postbump": "node scripts/bump-native-versions.js && git add android/app/build.gradle ios/App/App/Info.plist"
    }
  },
  "elf": {
    "cli": {
      "repoTemplate": "class",
      "plugins": [
        "@ngneat/elf-cli-ng"
      ],
      "repoLibrary": "src/app/store"
    }
  }
}
